oard = () => {\r\n    const [Items, setItems] = useState([])\r\n    const [Status, setStatus] = useState(\"loading\")\r\n\r\n    useEffect(async () => {\r\n        const response = await axios.get(`/api/items/loadItems`)\r\n        if (response && response.status === 200) {\r\n            setItems(response.data)\r\n            setStatus(\"OK\")\r\n        }\r\n    }, [])\r\n    return (\r\n        <div>\r\n            <div className={styles.grid}>\r\n                {Items.map(item => <SingleItem item={item} />)}\r\n            </div>\r\n            {Status == \"loading\" && <Loader />}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Dashboard","D:\\Code\\react\\School\\front-end\\src\\Containers\\StorePage\\Grid\\SingleItem\\SingleItem.js",["232","233","234"],"import React, {useEffect} from 'react'\r\nimport styles from './SingleItem.module.css'\r\n\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\r\nimport { Link } from 'react-router-dom'\r\n\r\n\r\nconst SingleItem = (props) => {\r\n    useEffect(() => {\r\n        console.log(props.item)\r\n    }, [])\r\n    return (\r\n        <Link to={`/items/${props.item._id}`} style={{ textDecoration: 'none', color: 'black', width: '100%' }}>\r\n            <div key={props.item._id} className={styles.innerContainer}>\r\n                <img src={props.item.thumbnail} className={styles.image}/>\r\n                <div className={styles.titleContainer}>\r\n                    <h3>{props.item.title}</h3>\r\n                    <h3 className={styles.price}>{`${parseFloat(props.item.price / 100)} €`}</h3>\r\n                </div>\r\n            </div>\r\n        </Link>\r\n    )\r\n}\r\n\r\nexport default SingleItem","D:\\Code\\react\\School\\front-end\\src\\Containers\\SingleItem\\SingleItem.js",["235","236","237"],"import React, { useEffect, useState } from 'react'\r\nimport { useParams } from \"react-router\";\r\nimport axios from 'axios'\r\nimport styles from './SingleItem.module.css'\r\nimport Item